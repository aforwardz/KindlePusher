<!--pages/account/account.wxml-->
<view class="container">
    <!-- 用户登录测试 -->
    <view class="userinfo" bindtap="login">
        <image
            class="userinfo-avatar"
            src="{{logged ? userInfo.avatar : '../../assets/images/user-unlogin.png'}}"
            background-size="cover"
        ></image>
        <view>
            <text class="userinfo-nickname">{{logged ? userInfo.nickname : '点击登录'}}</text>
        </view>
    </view>
    <view class='userdata' bindtap='login'>
      <text class='data-item' wx:for="{{userData}}" wx:key="k">{{item.v}}</text>
    </view>
</view>